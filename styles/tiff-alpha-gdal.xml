<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="epsg:3857" background-color="red">
  <Parameters>
    <Parameter name="sizes">600,400</Parameter>
  </Parameters>

<Style name="white">
  <Rule>
    <RasterSymbolizer opacity="1" scaling="bilinear" comp-op="src-over"/>
  </Rule>
</Style>
<Layer name="white"
  srs="epsg:3857">
    <StyleName>white</StyleName>
    <Datasource>
       <Parameter name="file">../../data/raster/white-alpha-assoc-alpha-correct.tiff</Parameter>
       <Parameter name="type">gdal</Parameter>
    </Datasource>
  </Layer>

</Map>
