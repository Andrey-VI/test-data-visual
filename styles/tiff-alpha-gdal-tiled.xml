<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="epsg:3857" background-color="blue">
  <Parameters>
    <Parameter name="sizes">400,600</Parameter>
  </Parameters>

<Style name="red">
  <Rule>
    <RasterSymbolizer opacity="1" scaling="bilinear" comp-op="src-over"/>
  </Rule>
</Style>
<Layer name="red"
  srs="epsg:3857">
    <StyleName>red</StyleName>
    <Datasource>
       <Parameter name="file">../../data/raster/red-square-transparent-preprocessed.tif</Parameter>
       <Parameter name="type">gdal</Parameter>
    </Datasource>
  </Layer>

</Map>
