<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="epsg:3857" background-color="blue">
  <Parameters>
    <Parameter name="sizes">600,400</Parameter>
  </Parameters>

<Style name="transp">
  <Rule>
    <RasterSymbolizer opacity="1" scaling="bilinear" />
  </Rule>
</Style>
<Layer name="transp"
  srs="epsg:3857">
    <StyleName>transp</StyleName>
    <Datasource>
       <Parameter name="file">../../data/raster/transp.tiff</Parameter>
       <Parameter name="type">gdal</Parameter>
    </Datasource>
  </Layer>

</Map>
